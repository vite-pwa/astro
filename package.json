    }
  },
  "dependencies": {
    "vite-plugin-pwa": "^1.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@antfu/ni": "^0.21.12",
    "@types/debug": "^4.1.8",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "astro": "^5.0.9",
    "bumpp": "^9.2.0",
    "eslint": "^9.23.0",
    "eslint-plugin-astro": "^1.3.1",
    "esno": "^4.0.0",
    "https-localhost": "^4.7.1",
    "typescript": "^5.7.2",
    "unbuild": "^3.0.1",
    "vite": "^5.0.0"
